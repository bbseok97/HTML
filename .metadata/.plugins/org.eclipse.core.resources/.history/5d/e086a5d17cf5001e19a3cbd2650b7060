<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>

<%
	//	사용자 인증 처리하는 세션을 삭제
	session.removeAttribute("user_id");

	//	세션 자체를 삭제
	session.invalidate();
%>
